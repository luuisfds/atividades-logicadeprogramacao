programa { // Calcular a média entre três números
  funcao inicio() {
    real numero1, numero2, numero3, media
    escreva("Escreva três números: \n")
    leia(numero1, numero2, numero3)

    media = (numero1 + numero2 + numero3)/3

    escreva("A média aritmética entre os números é ", media)
  }
}
